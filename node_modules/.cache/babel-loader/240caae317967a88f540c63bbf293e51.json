MAAhB,EAAwB;AACtB,UAAA,OAAO,CAAC,GAAR,CAAY,IAAZ;AACA,UAAA,IAAI,CAAC,UAAL,GAAkB,CAAC,IAAI,CAAC,UAAxB;AACF;;AACA,eAAO,IAAP;AACD,OANY,CAAb;AAOA,MAAA,YAAY,CAAC,OAAb,CAAqB,OAArB,EAA8B,IAAI,CAAC,SAAL,CAAe,KAAf,CAA9B;AACD,KAzBM;AA0BP,IAAA,SA1BO,qBA0BG,MA1BH,EA0BW;AAChB,UAAI,KAAI,GAAI,IAAI,CAAC,KAAL,CAAW,YAAY,CAAC,OAAb,CAAqB,OAArB,CAAX,KAA6C,EAAzD;AAEA,MAAA,OAAO,CAAC,GAAR,CAAY,YAAZ;AACA,WAAK,KAAL,GAAa,KAAK,KAAL,CAAW,MAAX,CAAkB,UAAC,IAAD;AAAA,eAAU,IAAI,CAAC,EAAL,KAAY,MAAtB;AAAA,OAAlB,CAAb;AACA,MAAA,YAAY,CAAC,OAAb,CAAqB,OAArB,EAA8B,IAAI,CAAC,SAAL,CAAe,KAAf,CAA9B;AACD;AAhCM;AAbI,CAAf","sourcesContent":["<template>\n  <div>\n    <input-todo @addTodo=\"addNewTodo\" />\n    <todo-container :todos=\"todos\" @setDone=\"setDone\" @deleteOne=\"deleteOne\" />\n    <login-form :todos=\"todos\" />\n  </div>\n</template>\n\n<script>\nimport todoContainer from \"../components/todoContainer\";\nimport InputTodo from \"../components/InputTodo\";\nimport LoginForm from \"./LoginForm\";\n\nexport default {\n  name: \"TodoList\",\n  components: {\n    todoContainer,\n    InputTodo,\n    LoginForm,\n  },\n  data() {\n    return {\n      newTodo: \"\",\n      todos: [],\n    };\n  },\n  methods: {\n    //payload is input newTodo\n    addNewTodo(payload) {\n      let todos = JSON.parse(localStorage.getItem(\"todos\")) || [];\n\n      this.todos.push({\n        id: this.todos.length + 1,\n        title: payload,\n        isComplete: false,\n      });\n\n      localStorage.setItem(\"todos\", JSON.stringify(todos));\n    },\n    setDone(todoId) {\n      let todos = JSON.parse(localStorage.getItem(\"todos\")) || [];\n\n      console.log(\"Set Done\");\n      this.todos = this.todos.map((todo) => {\n        if (todo.id === todoId) {\n          console.log(todo);\n          todo.isComplete = !todo.isComplete;\n        }\n        return todo;\n      });\n      localStorage.setItem(\"todos\", JSON.stringify(todos));\n    },\n    deleteOne(todoId) {\n      let todos = JSON.parse(localStorage.getItem(\"todos\")) || [];\n\n      console.log(\"Delete One\");\n      this.todos = this.todos.filter((todo) => todo.id !== todoId);\n      localStorage.setItem(\"todos\", JSON.stringify(todos));\n    },\n  },\n};\n</script>\n\n<style scoped>\n.isComplete {\n  text-decoration: black;\n}\n</style>"],"sourceRoot":""}]}