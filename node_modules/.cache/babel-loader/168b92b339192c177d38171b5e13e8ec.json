E,OALF,EAKE;AAJA,IAAA,IAAI,EAAC,UAIL;AAHA,IAAA,WAAW,EAAC,UAGZ;;aAFS,KAAA,CAAA,SAAA,CAAU,IAAV,GAAc,M;MAEvB;AADC,IAAA,UAAQ,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,SAAA,CAAA,UAAA,MAAA;AAAA,aAAQ,QAAA,CAAA,KAAA,EAAR;AAAA,KAAA,EAAa,CAAA,OAAA,CAAb,CAAA;AACT,GALF,E,IAAA,E;;AAAA,G,iBAGW,KAAA,CAAA,SAAA,CAAU,I,GAGjB,CAPN,C,EAQA,YAAA,CAAyC,QAAzC,EAAyC;AAAhC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,KAAA,EAAF;AAAA,KAAA;AAA2B,GAAzC,EAAyB,SAAzB,C,EACA,U","sourcesContent":["<template>\n  <h1>Login</h1>\n  <div>\n    <input\n      type=\"text\"\n      placeholder=\"Enter your username\"\n      v-model=\"userInput.name\"\n      @keypress.enter=\"login()\"\n    />\n  </div>\n  <div>\n    <input\n      type=\"password\"\n      placeholder=\"Password\"\n      v-model=\"userInput.pass\"\n      @keypress.enter=\"login()\"\n    />\n  </div>\n  <button @click=\"login()\">Sign in</button>\n  <p>\n    Don't have account? Let's\n    <a href=\"#\">Sign up</a>\n  </p>\n</template>\n\n<script>\nexport default {\n  name: \"LoginForm\",\n  data() {\n    return {\n      userInput: {\n        username: \"\",\n        password: \"\",\n      },\n    };\n  },\n  methods: {\n    login: function () {\n      let users = JSON.parse(localStorage.getItem(\"users\"));\n      if (!users) {\n        users = [];\n      }\n      this.errors = [];\n      this.messages = [];\n\n      let getUser = users.find((user) => {\n        return user.name === this.userInput.name;\n      });\n      if (getUser) {\n        this.messages.push(\"Loggin success!, welcomeback \" + getUser.name);\n        this.userLogin = getUser;\n        let todos = JSON.parse(localStorage.getItem(\"todos\"));\n        if (!todos) {\n          todos = [];\n        }\n        this.todos = todos.filter((todo) => todo.userId === this.userLogin.id);\n      } else {\n        this.errors.push(\"Wrong username!\");\n      }\n    },\n  },\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":""}]}