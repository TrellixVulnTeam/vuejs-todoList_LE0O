do,\n  },\n  data() {\n    return {\n      newTodo: \"\",\n      todos: [\n        { id: 1, title: \"Sleep\", isComplete: false },\n        { id: 2, title: \"Pay\", isComplete: false },\n        { id: 3, title: \"asd\", isComplete: false },\n        { id: 4, title: \"asd\", isComplete: false },\n        { id: 5, title: \"asd\", isComplete: false },\n        { id: 6, title: \"asd\", isComplete: false },\n      ],\n    };\n  },\n  methods: {\n    addNewTodo(payload) {\n      console.log(this.todos);\n      this.todos.push({\n        id: this.todos.length + 1,\n        title: payload,\n        isComplete: false,\n      });\n    },\n    setDone(todoId) {\n      this.todos = this.todos.map((todo) => {\n        if (todo.id === todoId) {\n          todo.isComplete = !todo.isComplete;\n        }\n      });\n    },\n  },\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":""}]}