sh(\"Loggin success!, welcomeback \" + getUser.name);\n        this.userLogin = getUser;\n        let todos = JSON.parse(localStorage.getItem(\"todos\"));\n        if (!todos) {\n          todos = [];\n        }\n        this.todos = todos.filter((todo) => todo.userId === this.userLogin.id);\n      } else {\n        this.errors.push(\"Wrong username!\");\n      }\n    },\n  },\n};\n</script>\n\n<style>\n</style>"]}]}