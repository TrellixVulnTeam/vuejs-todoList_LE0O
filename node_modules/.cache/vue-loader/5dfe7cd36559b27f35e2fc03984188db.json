AAC,CAAC","file":"/Users/Hung/hungg/vue-cli/login-form/src/pages/TodoList.vue","sourceRoot":"","sourcesContent":["<template>\n  <div>\n    <input-todo @addTodo=\"addNewTodo\" />\n    <todo-container :todos=\"todos\" @setDone=\"setDone\" @deleteOne=\"deleteOne\" />\n    <login-form :todos=\"todos\" />\n  </div>\n</template>\n\n<script>\nimport todoContainer from \"../components/todoContainer\";\nimport InputTodo from \"../components/InputTodo\";\nimport LoginForm from \"./LoginForm\";\n\nexport default {\n  name: \"TodoList\",\n  components: {\n    todoContainer,\n    InputTodo,\n    LoginForm,\n  },\n  data() {\n    return {\n      newTodo: \"\",\n      todos: [],\n    };\n  },\n  methods: {\n    //payload is input newTodo\n    addNewTodo(payload) {\n      let todos = JSON.parse(localStorage.getItem(\"todos\")) || [];\n\n      this.todos.push({\n        id: this.todos.length + 1,\n        title: payload,\n        isComplete: false,\n      });\n\n      localStorage.setItem(\"todos\", JSON.stringify(todos));\n    },\n    setDone(todoId) {\n      let todos = JSON.parse(localStorage.getItem(\"todos\")) || [];\n\n      console.log(\"Set Done\");\n      this.todos = this.todos.map((todo) => {\n        if (todo.id === todoId) {\n          console.log(todo);\n          todo.isComplete = !todo.isComplete;\n        }\n        return todo;\n      });\n      localStorage.setItem(\"todos\", JSON.stringify(todos));\n    },\n    deleteOne(todoId) {\n      let todos = JSON.parse(localStorage.getItem(\"todos\")) || [];\n\n      console.log(\"Delete One\");\n      this.todos = this.todos.filter((todo) => todo.id !== todoId);\n      localStorage.setItem(\"todos\", JSON.stringify(todos));\n    },\n  },\n};\n</script>\n\n<style scoped>\n.isComplete {\n  text-decoration: black;\n}\n</style>"]}]}