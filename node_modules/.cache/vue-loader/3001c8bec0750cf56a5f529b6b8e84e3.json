ntent":["<template>\n  <div class=\"login-form\">\n    <h1>Login</h1>\n    <div>\n      <input\n        type=\"text\"\n        placeholder=\"Enter your username\"\n        v-model=\"userInput.name\"\n        @keypress.enter=\"login()\"\n      />\n    </div>\n    <div>\n      <input\n        type=\"password\"\n        placeholder=\"Password\"\n        v-model=\"userInput.pass\"\n        @keypress.enter=\"login()\"\n      />\n    </div>\n    <div><button @click=\"login()\">Sign in</button></div>\n    <div>\n      <p>\n        Don't have account? Let's\n        <router-link to=\"/register\">Sign up</router-link>\n      </p>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"LoginForm\",\n  data() {\n    return {\n      userInput: {\n        name: \"\",\n      },\n      errors: [],\n      messages: [],\n    };\n  },\n  methods: {\n    login() {\n      let users = JSON.parse(localStorage.getItem(\"users\"));\n      if (!users) {\n        users = [];\n      }\n      this.errors = [];\n      this.messages = [];\n\n      let getUser = users.find((user) => {\n        return user.name === this.userInput.name;\n      });\n      setTimeout(() => {\n        if (getUser) {\n          this.messages.push(\"Loggin success!, welcomeback \" + getUser.name);\n          this.userLogin = getUser;\n          let todos = JSON.parse(localStorage.getItem(\"todos\"));\n          if (!todos) {\n            todos = [];\n          }\n          this.todos = todos.filter(\n            (todo) => todo.userId === this.userLogin.id\n          );\n        } else {\n          this.errors.push(\"Wrong username!\");\n        }\n      }, 1000);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.login-form {\n  text-align: center;\n}\n.login-form div {\n  margin-top: 1rem;\n}\n.login-form input {\n  font-size: 1rem;\n  border-radius: 0.5rem;\n}\n</style>"]}]}